<Window x:Class="genomgång2.MainWindow" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:local="clr-namespace:genomgång2" mc:Ignorable="d" Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Label Margin="10" FontSize="25">Meddelande</Label>
        <StackPanel Orientation="Horizontal">
            <Label Margin="10">Ange en text</Label>
            <TextBox Margin="5" Name="textRuta" Width="300"></TextBox>
        </StackPanel>
        <Button Click="Click_OK" Margin="5" Width="100">Ok</Button>
        <TextBox Name="rutaMeddelande" IsReadOnly="True" Height="200" Width="300"></TextBox>
    </StackPanel>
</Window>